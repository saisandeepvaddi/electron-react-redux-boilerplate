{
  "name": "my-electron-react-boilerplate",
  "version": "1.0.0",
  "description": "Electron boilerplate with React Redux electron-settings preconfigured. Entire basic redux directories created just for you.",
  "main": "electron.main.js",
  "scripts": {
    "start": "electron .",
    "webpack:dev": "webpack -d --watch",
    "webpack:prod": "sudo yarn clean:dist && webpack -p",
    "clean:dist": "rimraf ./dist/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saisandeepvaddi/my-electron-react-boilerplate.git"
  },
  "keywords": [
    "boilerplate",
    "electron",
    "react",
    "redux",
    "electron-settings"
  ],
  "author": "Sai Sandeep Vaddi <saisandeepvaddi@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/saisandeepvaddi/my-electron-react-boilerplate/issues"
  },
  "homepage": "https://github.com/saisandeepvaddi/my-electron-react-boilerplate#readme",
  "dependencies": {
    "electron-settings": "^3.1.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "css-loader": "^0.28.4",
    "devtron": "^1.4.0",
    "electron-reload": "^1.2.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.29.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.0.0",
    "webpack-bundle-analyzer": "^2.8.2"
  }
